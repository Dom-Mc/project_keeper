<% if @user.errors.any?%>
  <div id="error_explanation">
    <div class="alert alert-danger">
      <strong>Please fix the following error(s):</strong>
      <ul>
        <% @users.errors.full_messages.each do |error_message| %>
          <li><%= error_message %></li>
        <% end %>
      </ul>
    </div>
  </div>
<% end %>
