<h1>Create a new project</h1>

<form action="/projects" method="post">

  <label for="project_name">Name:</label>
  <input type="text" id="project_name" name="project[name]"><br>

  <label for="project_description">Description:</label>
  <input type="text" id="project_description" name="project[description]"><br>

  <label for="project_date_completed">Date Completed:</label>
  <input type="text" id="project_date_completed" name="project[date_completed]"><br>

  <input type="submit" value="Create Project">

</form>
